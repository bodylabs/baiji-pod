#!/usr/bin/env python
#
# If you wish to inject your own configuration, make a copy of this file.
#

def main():
    import sys
    from baiji.pod import StaticCache
    from baiji.pod.runners.sc_runner import SCRunner

    sc = StaticCache.create_default()

    runner = SCRunner(sc)
    return_status = runner.main()

    sys.exit(return_status)

if __name__ == '__main__':
    main()
